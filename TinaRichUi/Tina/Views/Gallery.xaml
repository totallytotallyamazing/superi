<navigation:Page 
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	xmlns:navigation="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Navigation"
	xmlns:controlsToolkit="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Toolkit"
	xmlns:System="clr-namespace:System;assembly=mscorlib"
	x:Class="Tina.Gallery" 
	Title="Gallery Page"
	d:DesignWidth="800" d:DesignHeight="1580" Loaded="Page_Loaded">
		<Grid x:Name="LayoutRoot" Height="1580" Width="800">
			<Grid.Background>
				<ImageBrush Stretch="Fill" ImageSource="/Tina;Component/Assets/galleryBg.jpg"/>
			</Grid.Background>
			<controlsToolkit:WrapPanel x:Name="panel" HorizontalAlignment="Center" Margin="0,110,0,0" VerticalAlignment="Top" Width="576"/>
			<controlsToolkit:BusyIndicator x:Name="Busy" HorizontalAlignment="Center" Margin="0,325,0,0"  VerticalAlignment="Top" Content="BusyIndicator" BusyContent="Загружается галарея" Visibility="Collapsed"/>
		</Grid>
	</navigation:Page>