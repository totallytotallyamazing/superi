        					</DoubleAnimationUsingKeyFrames>
        					<DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="mediaElement" Storyboard.TargetProperty="(FrameworkElement.Width)">
        						<EasingDoubleKeyFrame KeyTime="00:00:00" Value="400"/>
        						<EasingDoubleKeyFrame KeyTime="00:00:00.5000000" Value="800"/>
        					</DoubleAnimationUsingKeyFrames>
        					<DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="mediaElement" Storyboard.TargetProperty="(FrameworkElement.Height)">
